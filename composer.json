{
    "name": "skywest/ss-gallery",
    "description": "SilverStripe Gallery Module",
    "type": "library",
    "keywords": ["silverstripe", "image gallery", "gallery"],
    "authors": [{
        "name": "Hilkiah Makemo",
        "email": "hilmak01@gmail.com"
    }],
    "require": {
        "silverstripe/framework": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "SkyWest\\Gallery\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/PHPUnit": "^5.7"
    },
    "extra": {
